

local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))() local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))() local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Fluent:CreateWindow({ Title = "Script hub -by dead", SubTitle = "number 1 seals glazer", TabWidth = 160, Size = UDim2.fromOffset(580, 460), Acrylic = true, Theme = "Dark", MinimizeKey = Enum.KeyCode.LeftControl })

local function AddScriptButton(tab, title, code) tab:AddButton({ Title = title, Description = "Executes: " .. title, Callback = function() local success, err = pcall(function() loadstring(code)() end) if not success then Fluent:Notify({ Title = "Error", Content = tostring(err), Duration = 5 }) else Window:Dialog({ Title = "Executed", Content = "Destroy the UI?", Buttons = { { Title = "Yes", Callback = function() Fluent:Destroy() end }, { Title = "No", Callback = function() end } } }) end end }) end

