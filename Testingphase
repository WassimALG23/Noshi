for game, data in pairs(games) do local tab = Window:AddTab({ Title = game, Icon = data.icon }) for _, script in ipairs(data.scripts) do AddScriptButton(tab, script[1], script[2]) end end

local main = Window:AddTab({ Title = "Main", Icon = "book" }) main:AddParagraph({ Title = "Welcome", Content = "This is the Script Hub UI made by dead. All game tabs are pre-filled." })

local settings = Window:AddTab({ Title = "Settings", Icon = "settings" }) InterfaceManager:SetLibrary(Fluent) SaveManager:SetLibrary(Fluent) InterfaceManager:SetFolder("ScriptHub") SaveManager:SetFolder("ScriptHub/Configs") InterfaceManager:BuildInterfaceSection(settings) SaveManager:BuildConfigSection(settings) SaveManager:IgnoreThemeSettings() SaveManager:SetIgnoreIndexes({}) SaveManager:LoadAutoloadConfig()

Window:SelectTab(1) Fluent:Notify({ Title = "Script hub", Content = "Loaded successfully!", Duration = 8 })

